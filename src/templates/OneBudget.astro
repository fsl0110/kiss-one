---
import { oneBudget } from "@data/oneBudget";
import BlueprintItem from "@components/BlueprintItem.astro";
---

<div>
  <BlueprintItem
    name="Arms"
    thought={oneBudget.name}
    description={oneBudget.description}
    isTopthought={true}
    type="TopThought"
    isAnchor={true}
  />
  <BlueprintItem
    name="Arms - Arms"
    thought={oneBudget.arms.name}
    description={oneBudget.arms.description}
    topThought={oneBudget.name}
    base={oneBudget.name}
    type="SubThought"
  />
  <BlueprintItem
    name="Arms - Arms - Arms"
    thought={oneBudget.arms.arms.name}
    description={oneBudget.arms.arms.description}
    topThought={oneBudget.arms.name}
    base={oneBudget.name}
    type="DetailThought"
  />
  <BlueprintItem
    name="Arms - Arms - Left Leg"
    thought={oneBudget.arms.leftLeg.name}
    description={oneBudget.arms.leftLeg.description}
    topThought={oneBudget.arms.name}
    base={oneBudget.name}
    type="DetailThought"
  />
  <BlueprintItem
    name="Arms - Arms - Right Leg"
    thought={oneBudget.arms.rightLeg.name}
    description={oneBudget.arms.rightLeg.description}
    topThought={oneBudget.arms.name}
