    name="Arms - Right Leg - Left Leg"
    thought={oneBudget.rightLeg.leftLeg.name}
    description={oneBudget.rightLeg.leftLeg.description}
    base={oneBudget.name}
    type="DetailThought"
  />
  <BlueprintItem
    name="Arms - Right Leg - Right Leg"
    thought={oneBudget.rightLeg.rightLeg.name}
    description={oneBudget.rightLeg.rightLeg.description}
    base={oneBudget.name}
