    name="Arms - Left Leg - Arms"
    thought={oneBudget.leftLeg.arms.name}
    description={oneBudget.leftLeg.arms.description}
    topThought={oneBudget.leftLeg.name}
    base={oneBudget.name}
    type="DetailThought"
  />
  <BlueprintItem
    name="Arms - Left Leg - Left Leg"
    thought={oneBudget.leftLeg.leftLeg.name}
    description={oneBudget.leftLeg.leftLeg.description}
    topThought={oneBudget.leftLeg.name}
    base={oneBudget.name}
    type="DetailThought"
  />
  <BlueprintItem
    name="Arms - Left Leg - Right Leg"
    thought={oneBudget.leftLeg.rightLeg.name}
    description={oneBudget.leftLeg.rightLeg.description}
    topThought={oneBudget.leftLeg.name}
